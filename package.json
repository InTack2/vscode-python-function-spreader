{
  "name": "python-print-spreader",
  "displayName": "PythonPrintSpreader",
  "description": "You can easily add multiple Python print statements.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:python-print-spreader.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "python-print-spreader.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "PythonFunctionSpreader",
      "properties": {
        "python-print-spreader.target-function": {
          "type": "array",
          "default": [
            "print(\"${word}\", ${word})",
            "logger.info('${word}', ${word})"
          ],
          "description": "target language."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InTack2/vscode-python-function-spreader.git"
  },
  "author": "Tack2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/InTack2/vscode-python-function-spreader/issues"
  },
  "homepage": "https://github.com/InTack2/vscode-python-function-spreader#readme"
}